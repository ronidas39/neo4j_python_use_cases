    "import pandas as pd\n",
    "import io\n",
    "df=pd.read_excel(\"test.xlsx\")\n",
    "md5s=df[\"md5\"].unique()\n",
    "\n",
    "with io.open(\"data.csv\",\"w\",encoding=\"utf-8\")as f1:\n",
    " for md5 in md5s:\n",
    "  f1.write(md5+\",\"+\"|\".join(list(df.groupby(\"md5\")[\"file_path\"].get_group(md5)))+\"\\n\")"
   ]
